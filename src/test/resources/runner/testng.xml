<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="MyFork" parallel="classes" thread-count="3"  verbose="1" >
    <test name="Functional" >
        <classes>
            <class name="testcases.LoginTest" />
            <class name="testcases.ArrayDropDown"/>
            <class name="testcases.SignUpTest"/>
        </classes>
    </test>

    <test name="Regression">
        <classes>
            <class name="testcases.SignUpTest"/>
            <class name="testcases.ArrayDropDown"/>
            <class name="testcases.LoginTest"/>
        </classes>
    </test>
</suite>

